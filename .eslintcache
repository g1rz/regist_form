eact.useState('');\n    const [validPhone, setValidPhone] = React.useState(false);\n\n    const [userLang, setUserLang] = React.useState('');\n\n    const [pdn, setPdn] = React.useState(false);\n\n    const inputName = (text) => {\n        setUserName(text);\n        if (text.search(/[^A-Za-z_-]/g) === -1) {\n            setValidName(true);\n        } else {\n            setValidName(false);\n        }\n    };\n\n    const inputEmail = (text) => {\n        setUserEmail(text);\n        if (text.search(/^\\w+([.-]?\\w+)*@\\w+([.-]?\\w+)*(\\.\\w{2,3})+$/) !== -1) {\n            setValidEmail(true);\n        } else {\n            setValidEmail(false);\n        }\n    };\n\n    const inputPhone = (text) => {\n        setUserPhone(text);\n        if (\n            text.search(/^(\\+7|7|8)+\\(?([0-9]){3}\\)?-?[0-9]{3}-?([0-9]{4}|[0-9]{2}-?[0-9]{2})$/) !==\n            -1\n        ) {\n            setValidPhone(true);\n        } else {\n            setValidPhone(false);\n        }\n    };\n\n    const inputLang = (text) => {\n        setUserLang(text);\n    };\n\n    const inputPdn = (isCheck) => {\n        setPdn(isCheck);\n    };\n\n    const languages = [\n        {\n            id: 'ru',\n            name: 'Русский',\n        },\n        {\n            id: 'eng',\n            name: 'Английский',\n        },\n        {\n            id: 'cn',\n            name: 'Китайский',\n        },\n        {\n            id: 'sp',\n            name: 'Испанский',\n        },\n    ];\n\n    let isDisableSubmit = !(validName && validEmail && validPhone && userLang && pdn);\n    console.log(isDisableSubmit);\n    return (\n        <form className=\"r-form\">\n            <TextField\n                label=\"Имя\"\n                name=\"username\"\n                placeholder=\"Введите Ваше имя\"\n                fieldId=\"user_name\"\n                value={userName}\n                onChange={inputName}\n                isValid={validName}\n            />\n            <TextField\n                label=\"Email\"\n                name=\"username\"\n                placeholder=\"Введите ваш email\"\n                fieldId=\"user_email\"\n                value={userEmail}\n                onChange={inputEmail}\n                isValid={validEmail}\n            />\n            <TextField\n                label=\"Номер телефона\"\n                name=\"userphone\"\n                placeholder=\"Введите номер телефона\"\n                fieldId=\"user_phone\"\n                value={userPhone}\n                onChange={inputPhone}\n                isValid={validPhone}\n            />\n            <Dropdown\n                label=\"Язык\"\n                name=\"userlang\"\n                placeholder=\"Язык\"\n                fieldId=\"user_lang\"\n                list={languages}\n                onClick={inputLang}\n            />\n\n            <Check fieldId=\"pdn\" required={true} onClick={inputPdn}>\n                Принимаю <a href=\"#\">условия</a> использования\n            </Check>\n\n            <button className=\"r-form__btn\" type=\"submit\" disabled={isDisableSubmit}>\n                Зарегистрироваться\n            </button>\n        </form>\n    );\n};\n\nexport default RegistrForm;\n","/Users/sergeypeshkov/Documents/Проекты/React_study/registr_form/src/components/FieldWrap/FieldWrap.jsx",[],"/Users/sergeypeshkov/Documents/Проекты/React_study/registr_form/src/components/Dropdown/Dropdown.jsx",[],"/Users/sergeypeshkov/Documents/Проекты/React_study/registr_form/src/components/Check/Check.jsx",[],{"ruleId":"41","replacedBy":"42"},{"ruleId":"43","replacedBy":"44"},{"ruleId":"45","severity":1,"message":"46","line":12,"column":17,"nodeType":"47","endLine":12,"endColumn":47},{"ruleId":"45","severity":1,"message":"46","line":120,"column":26,"nodeType":"47","endLine":120,"endColumn":38},"no-native-reassign",["48"],"no-negated-in-lhs",["49"],"jsx-a11y/anchor-is-valid","The href attribute requires a valid value to be accessible. Provide a valid,